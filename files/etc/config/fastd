
config fastd 'default'
	option enabled 1
	option syslog_level 'info'
	list bind 'any'
	list method 'xsalsa20-poly1305'
	option mode 'tap'
	option interface 'fastd_mesh'
	option mtu 1426
	option forward 0
	option secret '' #set by /etc/init.d/freifunk_setup

config peer_group 'backbone'
	option enabled '1'
	option net 'default'
	option peer_limit '1'

config peer 'vpn1'
	option enabled 1
	option net 'default'
	option group 'backbone'
	option key 'd7822baec77e7f6bbcae9298b5506191e00f9eeaac1db4aba6e3c280678cac3c'
	list remote '"vpn1.talheim.net" port 1234'
	option float 0

config peer 'vpn2'
	option enabled 1
	option net 'default'
	option group 'backbone'
	option key '5369f843d41a89cbbcfe1839b1683e14869046252bb7af734f7d67b2711dd9cc'
	list remote '"vpn2.talheim.net" port 1234'
	option float 0

config peer 'vpn3'
	option enabled 1
	option net 'default'
	option group 'backbone'
	option key 'c1f20f98bf22860bbc43796c70387651c832c8e7c067761b262850567deab06e'
	list remote '"vpn3.talheim.net" port 1234'
	option float 0
